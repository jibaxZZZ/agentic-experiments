# Postgres
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/sql_agent

# OpenAI / Agent configuration (forwarded to sql_agent_llm)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-nano
OPENAI_API_BASE=

# MCP server (SSE)
